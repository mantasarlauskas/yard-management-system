@model IEnumerable<yard_management_system.Models.Ramp>

@{
    ViewData["Title"] = "Index";
}

<h2>Rampos</h2>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Code)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CategoryOfRamp)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Blocked)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BlockedFrom)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BlockedTo)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Code)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CategoryOfRamp)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Blocked)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BlockedFrom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BlockedTo)
            </td>
        </tr>
        }
    </tbody>
</table>
